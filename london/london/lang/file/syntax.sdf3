module lang/file/syntax

imports lang/base/syntax

sorts Program Prog
context-free syntax
  
  Program.Program = <
    <Prog>
  >
  
  Prog.Decls = <
    <{Decl "\n"}*>
  > 
  
  Prog.Value = <
    value <Val>
  >
  
  Decl.Exp = [>[Exp]] 
  
template options
  ID = keyword {reject}
  keyword -/- [a-zA-Z0-9]